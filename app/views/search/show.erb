<head>
    <meta charset="UTF-8">
    <title>My anime lists</title>
    <link rel="stylesheet" type="text/css" href="../css/search/results.css">
</head>


    <% @data.each do |d| %> 
        <% @image = d["cover"] %> 
        <% @title = d["title"] %> 
        <div>
            <img src="<%= @image %>" alt="<%= @title %>_image">
            <h3> <%= @title %> </h3>
        </div>
    <% end %> 
